# =============================================================================
# FRONTEND PRODUCTION ENVIRONMENT CONFIGURATION
# =============================================================================
# IMPORTANT: Replace all placeholder values with real production values

# =============================================================================
# API Configuration (Production)
# =============================================================================
VITE_API_URL=https://www.maxp-ai.pro/api/v1
VITE_WS_URL=wss://maxp-ai.pro

# =============================================================================
# Facebook SDK (Production)
# =============================================================================
VITE_FACEBOOK_APP_ID=your-production-facebook-app-id

# =============================================================================
# Google Services (Production)
# =============================================================================
VITE_GOOGLE_MAPS_API_KEY=your-production-google-maps-api-key

# =============================================================================
# Payment Gateways (Production)
# =============================================================================
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_your-production-stripe-publishable-key
VITE_PAYPAL_CLIENT_ID=your-production-paypal-client-id

# =============================================================================
# App Configuration (Production)
# =============================================================================
VITE_APP_NAME="Communication Platform"
VITE_APP_VERSION=1.0.0
VITE_COMPANY_NAME="Mokhtar Elhenawy"
VITE_BACKEND_URL=https://www.maxp-ai.pro

# =============================================================================
# Feature Flags (Production)
# =============================================================================
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_ECOMMERCE=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true

# =============================================================================
# File Upload (Production)
# =============================================================================
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf

# =============================================================================
# Theme Configuration (Production)
# =============================================================================
VITE_DEFAULT_THEME=light
VITE_PRIMARY_COLOR=#3B82F6
VITE_SECONDARY_COLOR=#10B981

# =============================================================================
# Analytics (Production)
# =============================================================================
VITE_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
VITE_MIXPANEL_TOKEN=your-production-mixpanel-token

# =============================================================================
# Error Tracking (Production)
# =============================================================================
VITE_SENTRY_DSN=your-production-sentry-dsn

# =============================================================================
# Development Settings (Production - Disabled)
# =============================================================================
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=error

# =============================================================================
# PWA Configuration (Production)
# =============================================================================
VITE_ENABLE_PWA=true
VITE_PWA_NAME="Communication Platform"
VITE_PWA_SHORT_NAME="CommPlatform"

# =============================================================================
# Localization (Production)
# =============================================================================
VITE_DEFAULT_LANGUAGE=ar
VITE_SUPPORTED_LANGUAGES=ar,en

# =============================================================================
# Real-time Features (Production)
# =============================================================================
VITE_ENABLE_REAL_TIME=true
VITE_HEARTBEAT_INTERVAL=30000

# =============================================================================
# Cache Configuration (Production)
# =============================================================================
VITE_CACHE_DURATION=300000

# =============================================================================
# WhatsApp Integration (Production)
# =============================================================================
VITE_WHATSAPP_BUSINESS_PHONE=your-production-whatsapp-phone

# =============================================================================
# WooCommerce Integration (Production)
# =============================================================================
VITE_WOOCOMMERCE_STORE_URL=https://your-woocommerce-store.com

# =============================================================================
# Security Settings (Production)
# =============================================================================
VITE_ENVIRONMENT=production
VITE_SECURE_MODE=true
